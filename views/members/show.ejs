<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
              <% include ../partials/head.ejs %>
  </head>
  <body>
    <header>
        <% include ../partials/header.ejs %>
    </header>
    <h1> <%=members.username%> </h1>
    show page
    <h1>Activity</h1>
    <ul>
      <% for (let i=0; i<members.posts.length; i++) { %>
        <li>
          <a href="/posts/show/<%=members.posts[i]._id%>">
            <%=members.posts[i].title%>
          </a>
        </li>
      <% } %>
    </ul>
    <section>
      <a href="/members/show/<%=members._id%>/edit">Edit Profile</a>
    </section>
    <section>
      <form action="/members/show/<%=members._id%>?_method=DELETE" method="POST">
        <input type="submit" value="Delete Member">
      </form>
    </section>
  </body>
</html>
